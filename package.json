{
  "name": "linq.js",
  "version": "1.0.6",
  "description": "Perform queries on collections in the manner of C#'s System.Linq in JavaScript",
  "keywords": [
    "linq",
    "linqjs",
    "collection",
    "util",
    "functional",
    "array",
    "iterable",
    "lazy"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo 'Starting build...' && grunt dist && echo 'Starting tests...' && ./node_modules/babel-cli/bin/babel-node.js ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- ./test/runner.js --reporter min --recursive && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "coverage": "grunt coverage",
    "build": "grunt",
    "generate-docs": "./node_modules/.bin/jsdoc --configure .jsdoc --verbose"
  },
  "author": "Sven Schmidt",
  "contributors": [
    {
      "name": "Gerriet Hinrichs"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SvSchmidt/linqjs.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-uglify": "^1.0.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-2017": "0.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^1.0.1",
    "grunt-file-info": "^1.0.12",
    "grunt-mocha-cli": "^2.1.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jscoverage": "^0.6.0",
    "jsdoc": "^3.4.1",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "typescript": "^2.4.1",
    "typedoc": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "gulp-typedoc": "^2.1.1",
    "gulp-uglify-es": "^0.1.7",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^5.0.1",
    "gulp-babel": "^7.0.0",
    "gulp-mocha": "^4.3.1",
    "del": "^3.0.0"
  }
}
