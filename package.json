{
  "name": "linq.js",
  "version": "1.0.6",
  "description": "Perform queries on collections in the manner of C#'s System.Linq in JavaScript",
  "keywords": [
    "linq",
    "linqjs",
    "collection",
    "util",
    "functional",
    "array",
    "iterable",
    "lazy"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo 'Starting build...' && gulp build && echo 'Starting tests...' && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- ./test/runner.js --report spec --recursive && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "coverage": "gulp coverage",
    "build": "gulp",
    "generate-docs": "gulp docs"
  },
  "author": "Sven Schmidt",
  "contributors": [
    {
      "name": "Gerriet Hinrichs"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SvSchmidt/linqjs.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "istanbul": "0.4.4",
    "jscoverage": "^0.6.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "typescript": "^2.4.1",
    "typedoc": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "gulp-typedoc": "^2.1.1",
    "gulp-uglify-es": "^0.1.7",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^5.0.1",
    "gulp-babel": "^7.0.0",
    "gulp-mocha": "^4.3.1",
    "gulp-sequence": "^0.4.6",
    "del": "^3.0.0"
  }
}
