{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src/"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|fragments|coverage|.git|test)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}
